package chap08;

import chap08.Accident.AccidentDetector;
import chap08.camera.SpeedCam;
import chap08.reporter.VideoReporter;

public class _Quiz_08 {

	public static void main(String[] args) {
		SpeedCam speedCam = new SpeedCam();
		speedCam.setDetector(new AccidentDetector());
		speedCam.setReporter(new VideoReporter());
		speedCam.detect();
		speedCam.report();
		

	}

}
