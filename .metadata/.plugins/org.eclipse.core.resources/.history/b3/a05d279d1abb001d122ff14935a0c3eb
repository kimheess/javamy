package chap_10;

public class _01_AnonymousClass1 {
	public static void main(String[] args) {
		//익명 클래스
		Coffee c1= new Coffee();
		c1.order("아메리카노");
		
		System.out.println("----------------");
		
		Coffee c2= new Coffee();
		c2.order("라떼");
		
		System.out.println("----------------");
	}
}

class Coffee{
	public void order(String coffee) {
		System.out.println("주문하신 " + coffee + " 나왔습니다.");
	}
}