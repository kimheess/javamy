package chap07;

public class Calc2 {
	private int num1;
	private int num2;
	
	public Calc2(){	
	}
	public void setNum1(int num1) {
		this.num1 = num1;
	}
	public void setNum2(int num2) {
		this.num2 = num2;
	}
	public int  getNum1() {
		return this.num1;
	}
	public int  getNum2() {
		return this.num2;
	}
	
	public int userResult(){
		int result = 0;
		
			result = num1 + num2;

		
		return result;
	}
}

