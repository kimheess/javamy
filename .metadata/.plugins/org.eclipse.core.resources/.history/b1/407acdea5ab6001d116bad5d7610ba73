	String script ="";

	if(session.isNew()){
		script += "alert('세션 객체가 생성되었습니다'!)";
	}
%>

<!DOCTYPE html>

<html>
<head>
   <meta charset="UTF-8">
   <title>session.jsp</title>
   <script>
   <%= script %>
   </script>
</head>

